    "@forge/storage": "^1.6.0",
    "@forge/ui": "^1.11.1",
    "@vueuse/components": "^11.3.0",
    "@vueuse/core": "^11.3.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vue-tsc": "^2.1.10"
  }
